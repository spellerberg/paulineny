/* ================ BASE ================ */

/* =======================================================================

Grid
  - Based on CSS Wizardry grid
========================================================================== */

@mixin grid($i) {

  .#{$i}m0 { margin: 0; }

  .#{$i}grid {
    @include clearfix;
    margin: 0;
    margin-left: -$grid-gutter;
    padding: 0;
    list-style: none;
    @include media-query($widescreen) {
      margin-left: -$grid-gutter__large;
    }
  }

  .#{$i}grid__item {
    float: left;
    width: 100%;
    padding-left: $grid-gutter;

    @include media-query($widescreen) {
      padding-left: $grid-gutter__large;
    }

    &[class*='--push'] {
      position: relative;
    }
  }

  .#{$i}grid--half-gutter {
    margin-left: (-$grid-gutter/2);

    .grid__item {
      padding-left: ($grid-gutter/2);
    }
  }

  /* Gutterless grids have all the properties of regular grids, minus any spacing. */
  .#{$i}grid--full {
    margin-left: 0;

    > .grid__item {
      padding-left: 0;
    }
  }

  /* Gutterless grids have all the properties of regular grids, minus any spacing. */
  .#{$i}grid--inline {
    text-align: center;

    > .grid__item {
      text-align: left;
      display: inline-block;
      float: none;
      vertical-align: top;
    }
  }


  /* ============================================================================
    Reversed grids allow you to structure your source in the opposite
    order to how your rendered layout will appear.
  ============================================================================== */
  .#{$i}grid--rev {
    text-align: left;
    direction: rtl;

    > .grid__item {
      float: right;
      text-align: left;
      direction: ltr;
    }
  }

}


@include grid('');

@each $breakpoint in $breakpoint-has-widths {
  @include media-query($breakpoint) {
    @include grid('#{$breakpoint}--');
  }
}

/* ============================================================================
  Grid Columns
    - Create width classes, prepended by the breakpoint name.
============================================================================== */

@mixin grid-column-generator($breakpoint: '') {
  /** Whole */
  .#{$breakpoint}one-whole {
    width: 100%;
  }

  /* Halves */
  .#{$breakpoint}one-half {
    width: percentage(1 / 2);
  }

  /* Thirds */
  .#{$breakpoint}one-third {
    width: percentage(1 / 3);
  }

  .#{$breakpoint}two-thirds {
    width: percentage(2 / 3);
  }

  /* Quarters */
  .#{$breakpoint}one-quarter {
    width: percentage(1 / 4);
  }

  .#{$breakpoint}two-quarters {
    width: percentage(2 / 4);
  }

  .#{$breakpoint}three-quarters {
    width: percentage(3 / 4);
  }

  /* Fifths */
  .#{$breakpoint}one-fifth {
    width: percentage(1 / 5);
  }

  .#{$breakpoint}two-fifths {
    width: percentage(2 / 5);
  }

  .#{$breakpoint}three-fifths {
    width: percentage(3 / 5);
  }

  .#{$breakpoint}four-fifths {
    width: percentage(4 / 5);
  }

  /* Sixths */
  .#{$breakpoint}one-sixth {
    width: percentage(1 / 6);
  }

  .#{$breakpoint}two-sixths {
    width: percentage(2 / 6);
  }

  .#{$breakpoint}three-sixths {
    width: percentage(3 / 6);
  }

  .#{$breakpoint}four-sixths {
