/* ================ COMPONENTS ================ */

/* =======================================================================

Responsive Images
========================================================================== */

.resp-img-wrapper {
  width: 100%;
  margin: 0 auto;

  &.js {
    position: relative;
  }
}

.resp-img {
  display: block;
  margin: 0 auto;
  background-color: transparent;
  transition: opacity 0.3s cubic-bezier(0.46, 0.01, 0.32, 1);
  opacity: 0;

  &.lazyloaded {
    opacity: 1;
  }
}

.resp-img-wrapper.js .resp-img,
.resp-img-wrapper.js .resp-img-placeholder {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}

.resp-img-wrapper.js .resp-img-placeholder {
  z-index: 0;
  opacity: 0.3;
  image-rendering: pixelated;
}

.resp-img-wrapper.js .resp-img--autoheight {
  height: auto;
}

.resp-img-wrapper {
  &.force-crop {
    overflow:hidden;
  }
}

.resp-img-wrapper.js.force-crop .resp-img {
  top: 0;
  left: 0;
  width: 100%;
  height: auto;
}

.resp-img[data-sizes='auto'] {
  display: block;
  width: 100%;
}

.placeholder-img-wrapper {
  svg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0.5;
  }
}


/* =======================================================================

Mobile Flickity
========================================================================== */

/* enable Flickity by default */
.mobile-flickity__slides::after {
  content: 'flickity';
  display: none; /* hide :after */
}

@include media-query($large-up) {
  /* disable Flickity for large devices */
  .mobile-flickity__slides::after {
    content: '';
  }
}


/* =======================================================================

Sections
========================================================================== */
//stylelint-disable
body:not(.banner-image) {
  .shopify-section:first-child section.bg--primary-background {
    padding-top: $space-2;
    @include media-query($large-up) {
      padding-top: $space-3;
    }
  }
}


/* =======================================================================

Mobile Navigation
========================================================================== */

.mobile-navigation {
  z-index: 1;
}

.mobile-navigation-container {
  transition: all 0.2s cubic-bezier(0.46, 0.01, 0.32, 1);
  width: 200vw;

  &.shift {
    left: -100%;
  }

  &.shift.mobile-navigation__pane:first-child {
    visibility: hidden;
  }
}


/* =======================================================================

Pagination
========================================================================== */

.page.current {
  border-bottom: 1px solid $color-primary-text;
}


.collection-item__button {
  .collection-item__link.color--white-text:hover &,
  .collection-item__link.color--primary-text:hover & {
    color: $color-primary-accent;
    border-bottom: 1px solid $color-primary-accent;
  }
  .collection-item__link.color--secondary-text:hover & {
    color: $color-secondary-accent;
    border-bottom: 1px solid $color-secondary-accent;
  }
  .collection-item__link.color--tertiary-text:hover & {
    color: $color-tertiary-accent;
    border-bottom: 1px solid $color-tertiary-accent;
  }
}
.product-item-hover {
  @include opacity(0);
  @include transition(all,0.2s,ease-in-out);
  &.show {
    @include opacity(1);
  }
}

/* ================ GLOBAL ================ */

/* =======================================================================

Slate reset
========================================================================== */

//stylelint-disable selector-no-qualifying-type

*,
*::before,
*::after {
  box-sizing: border-box;
}

body,
input,
textarea,
button,
select {
  -webkit-font-smoothing: antialiased;
  text-size-adjust: 100%;
}

a:focus {
  color: inherit;
}

img {
  max-width: 100%;
}

/* ================ Form element helpers ================ */
form {
  margin: 0;
}

// Prevent zoom on touch devices in active inputs
@include media-query($medium-down) {
  input,
  textarea,
  select {
    font-size: 16px;
  }
}

button,
input[type='submit'],
label[for] {
  cursor: pointer;
}

optgroup {
  font-weight: $font-weight-bold;
}

// Force option color (affects IE only)
option {
  background-color: $color-primary-background;
  color: $color-primary-text;
}

// hide outline on focus for elements which are given focus by JS
[tabindex='-1']:focus {
  outline: none;
}

/* ============================================================================
  Fast Tap
  enables no-delay taps (FastClick-esque) on supporting browsers
============================================================================== */
a,
button,
[role='button'],
input,
label,
select,
textarea {
  touch-action: manipulation;
}


/* =======================================================================

Layout
========================================================================== */

/* ================ General layout styles ================ */
body,
html {
  background-color: $color-primary-background;
}

.max-site-width {
  @include clearfix;
  max-width: $width-site;
  margin-left: auto;
  margin-right: auto;
}


/* =======================================================================

Icons
========================================================================== */

// stylelint-disable selector-no-qualifying-type
.icon-wrapper {
  display: inline-block;
}

.icon,
.icon-wrapper > svg {
  display: inline-block;
  vertical-align: middle;
  width: 2em;
  height: 2em;
  fill: currentColor;
}

.no-svg .icon {
  display: none;
}

.icon--wide {
  width: 40px;
}

// stylelint-disable max-nesting-depth
svg,
symbol {
  &.icon:not(.icon--full-color) {
    circle,
    ellipse,
    g,
    line,
    path,
    polygon,
    polyline,
    rect {
      fill: inherit;
      stroke: inherit;
    }
  }
}

/* ============================================================================
  A generic way to visually hide content while
  remaining accessible to screen readers (h5bp.com)
============================================================================== */
.icon-fallback-text {
  @include visually-hidden;

  .no-svg & {
    @include visually-shown(static);
  }
}

/* ================ Payment Icons ================ */
.payment-icons {
  @include prefix('user-select', 'none', moz ms webkit spec);
  cursor: default;
}

/* ================ Shopify icon on password page ================ */
.icon-shopify-logo {
  width: 1.5 * $font-size-base * 120 / 35;
  height: 1.5 * $font-size-base;
}


// css loading icon


@keyframes spinner {
  from {transform: rotate(0deg);}
  to {transform: rotate(360deg);}
}

.spinner::before {
  content: '';
  box-sizing: border-box;
  position: absolute;
  top: 50%;
  left: 50%;
  width: 20px;
  margin-top: -10px;
  margin-left: -10px;
  border-right: 2px solid transparent;
  animation: spinner 0.6s linear infinite;
}

.spinner.color--primary-text {
  &::before {
    border-top: 2px solid $color-primary-text;
  }
}

.spinner.color--active-text {
  &::before {
    border-top: 2px solid $color-active-text;
  }
}


/* =======================================================================

Rich Text Editor
========================================================================== */

.rte {
  font-family: $main-font-stack;

  img {
    height: auto;
  }

  table {
    table-layout: fixed;
  }

  ul,
  ol {
    margin-top: ($gutter);
    margin-bottom: ($gutter);
  }

}

// colors
.color--primary-text .rte {
  a {
    color: $color-primary-accent;
    border-bottom: 1px solid $color-primary-accent;
  }
}

.color--secondary-text .rte {
  a {
    color: $color-secondary-accent;
    border-bottom: 1px solid $color-secondary-accent;
  }
}

.color--tertiary-text .rte {
  a {
    color: $color-tertiary-accent;
    border-bottom: 1px solid $color-tertiary-accent;
  }
}

/* allow table to scroll for tables in the RTE since we don't know
   how many columns they will contain. Class added by JS. */
// sass-lint:disable no-misspelled-properties
.rte-table {
  overflow: auto;
  max-width: 100%;
  -webkit-overflow-scrolling: touch;
}

// This class is wrapped around YouTube/Vimeo embeds in the RTE
// to make them responsive and prevent layout breaking
.rte__video-wrapper {
  position: relative;
  overflow: hidden;
  max-width: 100%;
  height: 0;
  height: auto;
  padding-bottom: 56.25%;

  iframe {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }
}

// This class is wrapped around tables in the RTE
// to prevent layout breaking with a scrollable parent
.rte__table-wrapper {
  overflow: auto;
  max-width: 100%;
  -webkit-overflow-scrolling: touch;
}


/* =======================================================================

Responsive tables
========================================================================== */

@include media-query($small) {
  .responsive-table {
    width: 100%;

    thead {
      display: none;
    }

    tr {
      display: block;
    }

    // IE9 table layout fixes
    tr,
    td {
      float: left;
      clear: both;
      width: 100%;
    }

    th,
    td {
      display: block;
      margin: 0;
      padding: ($gutter / 3) 0;
      text-align: right;
    }

    td::before {
      content: attr(data-label);
      float: left;
      padding-right: 10px;
      text-align: center;
      font-weight: bold;
    }
  }

  // Add a keyline between rows
  .responsive-table-row + .responsive-table-row,
  tfoot > .responsive-table-row:first-child {
    position: relative;
    margin-top: 10px;
    padding-top: $gutter / 2;

    &::after {
      content: '';
      position: absolute;
      top: 0;
      right: 0;
      left: 0;
      display: block;
      border-bottom: 1px solid $color-primary-text;
    }
  }
}


/* =======================================================================

Links + Buttons
========================================================================== */

.btn,
.shopify-payment-button__button--unbranded {
  @extend %f--accent;
  @include prefix('user-select', 'none', moz ms webkit spec);
  @include prefix(appearance, none, webkit moz spec);

  vertical-align: middle;
  border-radius: 0;
  text-align: center;
  white-space: nowrap;
  text-decoration: none;
}

//stylelint-disable max-nesting-depth
body:not(.user-is-tabbing) {
  .btn,
  .shopify-payment-button__button--unbranded {
    &:focus {
      outline: 0;
    }
  }
}

a,
a:visited {
  text-decoration: none;
}

.link-underline {
  padding-bottom: 0.25rem;
}


/* ============================================================================
  Form scaffolding
    - Selectors setup for you to style form elements how you want
    - Focus, error, and disabled states are set to be extended
============================================================================== */

// stylelint-disable selector-no-qualifying-type

input,
textarea,
input[type='search'],
select {
  max-width: 100%;
  border-radius: 0;
  -webkit-appearance: none;
  -moz-appearance: none;
  -webkit-border-radius: 0;

  &[disabled] {
    border-color: $color-disabled-border;
    background-color: $color-disabled;
    cursor: default;
  }
}

.input-error {
  border-color: $color-error;
  background-color: $color-error-bg;
  color: $color-error;
}

textarea {
  min-height: 100px;
}

select::-ms-expand {
  display: none;
}

select {
  background-size: 18px 18px;

  background: {
    image: url('{{ "ico-select.svg" | asset_url }}');
    repeat: no-repeat;
    position: right 10px center;
  }
}

select.color--primary-text {
  background-image: url('{{ "ico-select__primary.svg" | asset_url }}');
}

select.color--secondary-text {
  background-image: url('{{ "ico-select__secondary.svg" | asset_url }}');
}

select.color--tertiary-text {
  background-image: url('{{ "ico-select__tertiary.svg" | asset_url }}');
}

select.color--white-text {
  background-image: url('{{ "ico-select__white.svg" | asset_url }}');
}

select.color--black-text {
  background-image: url('{{ "ico-select__black.svg" | asset_url }}');
}


/*================ Input Group ================*/
.input-group {
  display: table;
  border-collapse: separate;

  input::-moz-focus-inner {
    border: 0;
    padding: 0;
    margin-top: -1px;
    margin-bottom: -1px;
  }
}

.input-group-field,
.input-group-btn {
  display: table-cell;
  vertical-align: middle;
  margin: 0;
}

.input-group .btn,
.input-group .input-group-field {
  //height: 37px;
}

.input-group .input-group-field {
  width: 100%;
}

.input-group-btn {
  position: relative;
  white-space: nowrap;
  width: 1%;
  padding-left: 16px;
}



input[type='checkbox'] {
  -webkit-appearance: checkbox;
  appearance: checkbox;
}

input[type='radio'] {
  -webkit-appearance: radio;
  appearance: radio;
}


// keep outline for keyboard users
body:not(.user-is-tabbing) button:focus,
body:not(.user-is-tabbing) input:focus,
body:not(.user-is-tabbing) select:focus,
body:not(.user-is-tabbing) textarea:focus,
body:not(.user-is-tabbing) a:focus {
  outline: none;
}

// stylelint-disable selector-no-vendor-prefix
::-webkit-input-placeholder { /* Chrome/Opera/Safari */
  color: inherit;
}

::-moz-placeholder { /* Firefox 19+ */
  color: inherit;
}

:-ms-input-placeholder { /* IE 10+ */
  color: inherit;
}

:-moz-placeholder { /* Firefox 18- */
  color: inherit;
}

@mixin color-scheme-placeholder($color) {
  ::-webkit-input-placeholder { /* Chrome/Opera/Safari */
    color: $color;
  }

  ::-moz-placeholder { /* Firefox 19+ */
    color: $color;
  }

  :-ms-input-placeholder { /* IE 10+ */
    color: $color;
  }

  :-moz-placeholder { /* Firefox 18- */
    color: $color;
  }
}

.color--primary-text {
  @include color-scheme-placeholder($color-primary-text--faded);
}

.color--secondary-text {
  @include color-scheme-placeholder($color-secondary-text--faded);
}

.color--tertiary-text {
  @include color-scheme-placeholder($color-tertiary-text--faded);
}


// Errors
.errors {
  border: 1px solid $color-primary-accent;
  background-color: $color-primary-accent;
  color: $color-primary-accent--overlay;
  padding: 8px;
  margin: $space-2 0;

  // Shopify generates ul element
  ul {
    list-style: none;
    padding: 0;
    margin: 0;

  }

  a {
    color: $color-error;
    text-decoration: underline;
  }
}


/* =======================================================================

Blank States
========================================================================== */

$color-blankstate: rgba(#000, 0.35);
$color-blankstate-border: rgba(#000, 0.2);
$color-blankstate-background: rgba(#000, 0.1);

.placeholder-svg {
  display: block;
  width: 100%;
  max-width: 100%;
  height: 100%;
  max-height: 100%;
  border: 1px solid $color-blankstate-border;
  background-color: $color-blankstate-background;
  fill: $color-blankstate;
}

.placeholder-svg--small {
  width: 480px;
}

.placeholder-noblocks {
  padding: 40px;
  text-align: center;
}

// Mimic a background image by wrapping the placeholder svg with this class
.placeholder-background {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;

  .icon {
    border: 0;
  }
}


/* =======================================================================

Header Active (Modal)
========================================================================== */

.header__active {
  display: none;
  opacity: 0;
  transition: opacity 0.1s cubic-bezier(0.46, 0.01, 0.32, 1);
}


.header__shifter {
  height: 0;
  transition: all 0.2s cubic-bezier(0.46, 0.01, 0.32, 1);
}

.header__close {
  display: none;
  z-index: 50;
}

.site-header.full-height {
  height: 100vh;
}


.site-header.active {
  background-color: $active-background;
  color: $active-text;
  overflow-y: scroll;
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;

  @include media-query($large-up) {
    .header__shifter {
      height: 43px;
    }
  }

  @include media-query($medium-down) {
    .header-table {
      display: none;
    }
  }

  .header__close {
    display: block;
    transition: all 0.2s cubic-bezier(0.46, 0.01, 0.32, 1);
    svg {
      transition: all 0.2s cubic-bezier(0.46, 0.01, 0.32, 1);
    }
  }

  .header__close:hover {
    svg {
      transform: scale(1.2);
    }
  }

  .header__active {
    display: block;
  }

  .header__active.visible {
    opacity: 1;
  }

  .announcement-bar,
  .header__logo {
    visibility: hidden;
  }

  .header-table__center {
    //max-width: 50px;
    overflow: hidden;
  }

  .header-table,
  .header__active {
    z-index: 1;
  }

  .nav__container {
    overflow: unset;
  }

  .header__link.isSelected .header__link-wrapper,
  .header__link:hover .header__link-wrapper {
    border-bottom: 1px solid $active-text;
  }

  .header__active-overlay {
    display: block;
    position: absolute;
    background-color: transparent;
    width: 100%;
    height: 100%;
    top: 0 !important;
    left: 0;
    z-index: -1;
  }

  .header__meta {
    flex-grow: 0;
    flex-basis: auto;
  }

  .site-nav {
    white-space: normal;
  }
}

.site-header.active.has-announcement {
  .header__shifter {
    height: 0;
  }
}

// header active
.site-header.active {
  .color--header-active {
    color: $active-text;
  }

  .border-bottom--header--active {
    border-bottom: 1px solid $active-text;
  }
}


/* =======================================================================

No JS
========================================================================== */

//stylelint-disable
.no-js {
  .site-header {
    position: static;

    nav ul {
      white-space: normal;
    }
    .no-js--submenu {
      display: none;
    }
    .site-nav--has-submenu:hover .no-js--submenu {
      display: block;
      z-index: 100;
    }
  }

  .no-js__navigation {
    display: none;
  }

  .nav-open {
    .nav-open__close {
      display: none;
    }
  }

  #nav:checked + .nav-open {
    position: fixed;
    top: 20px;
    left: 20px;
    z-index: 10;

    .nav-open__default {
      display: none;
    }

    .nav-open__close {
      display: block;
    }
  }

  #nav:checked ~ .no-js__navigation {
    display: block;
    padding-top: 100px;
    overflow-y: scroll;
  }

  .item.fade-in {
    @include opacity(1);
    float: left;
  }

  .image-with-text__container .item--image {
    width: 100%;
  }
}

/* =======================================================================

Rich Text
========================================================================== */

.color--primary-text .richtext {
  a {
    color: $color-primary-text;
    border-bottom: 1px solid $color-primary-text;
  }

  a:hover {
    color: $color-primary-accent;
    border-bottom: 1px solid $color-primary-accent;
  }
}

.color--secondary-text .richtext {
  a {
    color: $color-secondary-text;
    border-bottom: 1px solid $color-secondary-text;
  }

  a:hover {
    color: $color-secondary-accent;
    border-bottom: 1px solid $color-secondary-accent;
  }
}

.color--tertiary-text .richtext {
  a {
    color: $color-tertiary-text;
    border-bottom: 1px solid $color-tertiary-text;
  }

  a:hover {
    color: $color-tertiary-accent;
    border-bottom: 1px solid $color-tertiary-accent;
  }
}


/* =======================================================================

Fade In
========================================================================== */


.fade-in {
  @include opacity(0);
  transition: all 0.6s cubic-bezier(0.46, 0.01, 0.32, 1);
  -webkit-transform: translateY(2rem);
  transform: translateY(2rem);
}

.activated.fade-in {
  @include opacity(1);
  -webkit-transform: translateY(0);
  transform: translateY(0);
}


/* ================ MODULES ================ */

/* =======================================================================

Gift Card Template
========================================================================== */

.giftcard-qr {
  img {
    display: block;
    margin: 0 auto;
  }
}

.apple-wallet-image {
  display: block;
  margin: 0 auto;
}

/* ================ Print Giftcard Styles ================ */
@media print {
  @page {
    margin: 0.5cm;
  }

  p {
    orphans: 3;
    widows: 3;
  }

  .print-giftcard,
  .apple-wallet {
    display: none;
  }
}


/* =======================================================================

Ajax Cart
========================================================================== */

.js-qty__num,
.ajaxcart__qty-num {
  height: 43px;

  .cart & {
    height: 35px;
  }
}

.cookie-message {
  display: none;
  padding-bottom: 25px;
}

@each $color in $color-strings {
  $i: index($color-strings, $color);

  .js-qty-wrapper.color--#{$color} {
    .js-qty {
      border: 1px solid nth($colors, $i);
    }

    button,
    input {
      color: nth($colors, $i);
    }
  }

  .ajax-qty-wrapper.color--#{$color} {
    .ajax-qty {
      border: 1px solid nth($colors, $i);
    }

    button,
    input {
      color: nth($colors, $i);
    }
  }
}


.cart__spinner {
  background-color: $color-active-background--faded;
}

.btn__ajax-checkout {
  &[disabled],
  &.disabled {
    @include opacity(0.3);

    background: transparent;
    border: 1px solid $color-active-text;
    color: $color-active-text;
  }
}

@include media-query($small) {
  .cart-page__qty {
    position: absolute;
    top: 40px;
    right: -15px;
    max-width: 100px;
  }
}


/* ================ SECTIONS ================ */

/* =======================================================================

Header
========================================================================== */

.color--header-active {
  @include transition(color, 0.2s,ease-in-out);
}

@include media-query($large-up) {
  .sticky-header-container {
    position: sticky;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 10;
    background-color: transparent;
    @include transition(background-color,0.2s,ease-in-out);
  }
}

.is-header-transitioning {
  // display: block !important;
  // visibility: visible !important;
  // height: 100vh !important;

}

.site-logo {
  display: inline-block;

  img {
    display: block;
    width: 100%;
  }
}

.site-header__menu-btn {
  height: 100%;
  line-height: 100%;

  .icon-wrapper {
    margin-left: -10px;
  }

  svg {
    vertical-align: -10px;
  }
}

.site-header {
  background-color: transparent;
  z-index: 10;
  transition: background-color 0.2s ease-in-out;
}


.header-table__center {
  padding-bottom: 0.3rem;
}

.header__link-wrapper {
  padding-bottom: 0.2rem;
  border-bottom: 1px solid transparent;
  font-size: 3.6rem;
  text-transform: lowercase;
}


.site-nav__svg {
  //padding-left: 0.5rem;
  padding-bottom: 0.1rem;
}

.cart__icon {
  margin-right: -0.3rem;
}

.header__active-overlay {
  display: none;
}

.banner__placeholder-svg {
  @include opacity(0.2);
  background-color: $placeholder-light;
  svg {
    width: 100%;
    height: 100%;
  }
}


@include media-query($large-up) {
  .header-static-color {
    color: $color-primary-text;
    &:hover .header__link-wrapper {
      border-color: $color-primary-text;
    }
  }
}


/* =======================================================================

Footer
========================================================================== */

.powered_by_link {
  a {
    color: inherit;
  }
}

.payment-icon {
  width: 40px;
  height: 28px;

  svg {
    display: block;
  }
}



.footer__nav-link-span {
  padding-bottom: 0.2rem;
  border-bottom: 1px solid transparent;

  .footer__nav-link.color--primary-text:hover & {
    border-bottom: 1px solid $color-primary-text;
  }
  .footer__nav-link.color--secondary-text:hover & {
    border-bottom: 1px solid $color-secondary-text;
  }
  .footer__nav-link.color--tertiary-text:hover & {
    border-bottom: 1px solid $color-tertiary-text;
  }
}

/* =======================================================================

Map
========================================================================== */

.map__container {
  position: relative;

  .placeholder-svg {
    border: 0;
    min-height: 300px;
  }
}


.map-wrapper {
  position: relative;
  padding-bottom: 66%;
  overflow: hidden;
}

.map-section__container {
  position: absolute;
  height: 100%;
  min-height: auto;
  left: 0;
  top: 0;
  width: 100%;
}



/* =======================================================================

Image with text
========================================================================== */

.image-with-text__placeholder-svg {
  @include opacity(0.2);
}


/* =======================================================================

Richtext
========================================================================== */

.richtext__content {
  column-count: 2;
  column-gap: 2em;

  p {
    display: inline-block;
    -webkit-column-break-inside: avoid;
    break-inside: avoid;
    page-break-inside: avoid;
  }
}


/* =======================================================================

Cascade
========================================================================== */

// stylelint-disable scss/dollar-variable-pattern

.cascading-content {
  overflow-x: hidden;
}

/* clear fix */
.items::after {
  content: '';
  display: block;
  clear: both;
}


/* GRID WIDTHS FOR MOBILE FIRST */
$container_mb: 335;
$columns_mb: 2;
$gutter_mb: 25;
$column-width_mb: 155;

$half_mb: $column-width_mb;
$full_mb: $container_mb;

$gutter_mb: percentage($gutter_mb / $container_mb);
$half_mb: percentage($half_mb / $container_mb);
$full_mb: percentage($full_mb / $container_mb);


/* GRID WIDTHS FOR DESKTOP */
$container_dt: 1280;
$columns_dt: 2;
$gutter_dt: 80;
$column-width_dt: 600;

//widths
$half_dt: $column-width_dt;
$full_dt: $container_dt;

//percentage widths
$gutter_dt: percentage($gutter_dt / $container_dt);
$half_dt: percentage($half_dt / $container_dt);
$full_dt: percentage($full_dt / $container_dt);

.items {
  overflow: hidden;
}

.item-sizer {
  width: $gutter_mb;
  @include media-query($large-up) {
    width: $gutter_dt;
  }
}

.column-sizer {
  @include media-query($medium-down) {
    width: $half_mb;
  }
  @include media-query($large-up) {
    width: $half_dt;
  }
}

.item {
  opacity: 0;
  padding-top: 60px;

  @include media-query($large-up) {
    padding-top: 120px;
  }

  &.ready {
    opacity: 1;
  }
}

.item--large-gap {
  @include media-query($large-up) {
    padding-top: 240px;
  }
}



//Mobile block sizes
@include media-query($medium-down) {
  .item--mobile--half {
    width: $half_mb;
  }

  .item--mobile--full {
    width: $full_mb;
    padding-bottom: 60px;
  }

  .first-col--mobile {
    padding-top: 0;
  }

  .item--mobile--full+.item--mobile--full {
    padding-top: 0;
  }
}

//Desktop block sizes
@include media-query($large-up) {
  .item--desktop--half {
    width: $half_dt;
  }

  .item--desktop--full {
    width: $full_dt;
    padding-bottom: 120px;

    &.item--large-gap {
      padding-bottom: 240px;
    }
  }

  .first-col--desktop {
    padding-top: 0;
  }

  .item--desktop--full+.item--desktop--full {
    padding-top: 0;
  }
}

.item--first {
  padding-top: 0;
  margin-top: 0;
}

.item--last {
  padding-bottom: 3px;
}

.item--heading {
  padding-bottom: 60px;
}



.item {
  &.fade-in {
    @include opacity(0);
    transition: all 0.4s cubic-bezier(0.46, 0.01, 0.32, 1);
    -webkit-transform: translateY(2rem);
    transform: translateY(2rem);
  }

  &.activated.fade-in.ready {
    @include opacity(1);
    -webkit-transform: translateY(0);
    transform: translateY(0);
  }
}


/* =======================================================================

Product
========================================================================== */


// Add to cart disabled states
// stylelint-disable selector-no-qualifying-type
// stylelint-disable max-nesting-depth

.btn--add-to-cart.color-scheme--primary {
  &[disabled],
  &.disabled {
    background: transparent;
    border: 1px solid $color-primary-text;
    color: $color-primary-text;
  }
}

.btn--add-to-cart.color-scheme--secondary {
  &[disabled],
  &.disabled {
    background: transparent;
    border: 1px solid $color-secondary-text;
    color: $color-secondary-text;
  }
}

.btn--add-to-cart.color-scheme--tertiary {
  &[disabled],
  &.disabled {
    background: transparent;
    border: 1px solid $color-tertiary-text;
    color: $color-tertiary-text;
  }
}

.btn--add-to-cart {
  &[disabled],
  &.disabled {
    @include opacity(0.3);
  }
}

.shopify-payment-button__button {
  margin: 0 auto;
}

.payment-button-wrapper {
  min-height: 48px;
}

.shopify-payment-button__button--unbranded {
  transition: none;

  &[disabled],
  &.disabled {
    cursor: default;
    display: none;
  }
}

.payment-button-wrapper.color--primary {
  .shopify-payment-button__button--unbranded {
    background-color: $color-primary-accent;
    color: $color-primary-accent--overlay;

    &:hover:not([disabled]) {
      background-color: $color-primary-text;
      color: $color-primary-text--overlay;
    }
  }
}

.payment-button-wrapper.color--secondary {
  .shopify-payment-button__button--unbranded {
    background-color: $color-secondary-accent;
    color: $color-secondary-accent--overlay;

    &:hover:not([disabled]) {
      background-color: $color-secondary-text;
      color: $color-secondary-text--overlay;
    }
  }
}


.payment-button-wrapper.color--tertiary {
  .shopify-payment-button__button--unbranded {
    background-color: $color-tertiary-accent;
    color: $color-tertiary-accent--overlay;

    &:hover:not([disabled]) {
      background-color: $color-tertiary-text;
      color: $color-tertiary-text--overlay;
    }
  }
}

.shopify-payment-button__button--unbranded,
.shopify-payment-button__button,
.shopify-payment-button__more-options {
  &[disabled],
  button[aria-disabled],
  &.disabled {
    cursor: default;
    display: none;
  }

}

.cta-content.hide-pay-buttons {
  .shopify-payment-button {
    display: none;
  }
}

// Flickity
/* enable Flickity by default */
.product-flickity__slides::after {
  content: 'flickity';
  display: none; /* hide :after */
}

@include media-query($medium-up) {
  /* disable Flickity for large devices */
  .product-flickity__slides::after {
    content: '';
  }
}

.product__thumbnails::-webkit-scrollbar { width: 0 !important }

/* =======================================================================

Gift Card
========================================================================== */

.giftcard__code {
  bottom: 30px;
  left: 0;
}

.giftcard__dotted {
  border: 1px dashed $color-primary-text;
  letter-spacing: 0.2em;
}

.collection__sticky-tags {
  .sticky-header & {
    top: 140px;
  }
}



// Pauline header

.site-header {  }
.site-header.black { background: #000; }

.site-header.active .header-table { display: none; }

.color--headerlogo { color: transparent;  }
.color--headerlogo.hidden { display: none; }
.color--headerlogo.header-static-color { color: #fff; }

.paulinelarge { position: absolute; top: 0; left: 0; right: 0; bottom: 0; display: flex; justify-content: center; align-items: center; font-family: "PrismasetOne",sans-serif; font-weight: 400; font-size: 15vw; line-height: 0; }

.header-table { padding: 1rem 2.5rem; }

// Pauline Footer

.footer-grid { display: grid; grid-template-columns: 1fr 1fr; grid-column-gap: 2rem; padding: 0 2.5rem; }
.footer-copyright { text-align: left; }
.footer-social { text-align: right; }

// Product

.productoption { border: none; margin-top: 2rem; padding: 0; }
.productoption legend { color: #999; }
.productoption input[type="radio"] { display: none; }
.productoption input[type="radio"]:checked+label { color: #999; }
.productoption label { margin-right: .25em; }
.paulineaddbutton { position: relative; font-family: "PrismasetOne",sans-serif; text-transform: lowercase; font-weight: 400; font-style: normal; background: transparent; border: none; padding: 0; margin-top: 1em; font-size: 2rem; border-bottom: 1px solid #fff; cursor: pointer; }

.paulineaddbutton::after,
.paulineaddbutton::before {
	content: attr(data-text);
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	transition: all 0.2s;
}
.paulineaddbutton:hover::after{
  top: -6px;
}
.paulineaddbutton:hover::before{
  top: -12px;
}

