/*================ UTILS ================*/
/* stylelint-disable declaration-no-important */

@mixin screen-scale-full {
  -webkit-transform: scale(1) rotate(0);
  -moz-transform: scale(1) rotate(0);
  -ms-transform: scale(1) rotate(0);
  -o-transform: scale(1) rotate(0);
  transform: scale(1) rotate(0);
}

@mixin transition($method, $sec, $timing) {
  /* transition */
  -webkit-transition: $method $sec $timing;
  -moz-transition: $method $sec $timing;
  -o-transition: $method $sec $timing;
  transition: $method $sec $timing;
}

@mixin translate($a, $b) {
  -webkit-transform: translate($a, $b);
  -moz-transform: translate($a, $b);
  -ms-transform: translate($a, $b);
  -o-transform: translate($a, $b);
  transform: translate($a, $b);
}


@mixin border-radius($size) {
  -webkit-border-radius: $size;
  border-radius: $size;
}

@mixin opacity($dec) {
  // scss-lint:disable SpaceAroundOperator
  -ms-filter: 'progid:DXImageTransform.Microsoft.Alpha(Opacity=" + ($dec * 100) + ")';
  filter: alpha(opacity=($dec * 100));
  -moz-opacity: $dec;
  -khtml-opacity: $dec;
  opacity: $dec;
}


/* ================ Mixins ================ */
@mixin clearfix() {
  // sass-lint:disable;
  *zoom: 1;

  &::after {
    content: '';
    display: table;
    clear: both;
  }
}

/* ============================================================================
  Prefix mixin for generating vendor prefixes.
  Based on https://github.com/thoughtbot/bourbon/blob/v4-stable/app/assets/stylesheets/addons/_prefixer.scss

  Usage:
    // Input:
    .element {
      @include prefix(transform, scale(1), ms webkit spec);
    }

    // Output:
    .element {
      -ms-transform: scale(1);
      -webkit-transform: scale(1);
      transform: scale(1);
    }
============================================================================== */
@mixin prefix($property, $value, $prefixes) {
  @each $prefix in $prefixes {
    @if $prefix == webkit {
      -webkit-#{$property}: $value;
    } @else if $prefix == moz {
      -moz-#{$property}: $value;
    } @else if $prefix == ms {
      -ms-#{$property}: $value;
    } @else if $prefix == o {
      -o-#{$property}: $value;
    } @else if $prefix == spec {
      #{$property}: $value;
    } @else {
      @warn 'Unrecognized prefix: #{$prefix}';
    }
  }
}

@mixin transform($transform) {
  @include prefixer(transform, $transform, ms webkit spec);
}

@mixin user-select($value: none) {
  @include prefixer(user-select, $value, webkit moz ms spec);
}

@mixin backface($visibility: hidden) {
  @include prefixer(backface-visibility, $visibility, webkit spec);
}

@function em($target, $context: $base-font-size) {
  @if $target == 0 {
    @return 0;
  }

  @return $target / $context + 0;
}
// scss-lint:disable ColorVariable
@function color-control($color) {
  @if (lightness( $color ) > 40) {
    @return #000;
  } @else {
    @return #fff;
  }
}

/* ================ Media Query Mixin ================ */
@mixin media-query($media-query) {
  $breakpoint-found: false;

  @each $breakpoint in $breakpoints {
    $name: nth($breakpoint, 1);
    $declaration: nth($breakpoint, 2);

    @if $media-query == $name and $declaration {
      $breakpoint-found: true;

      @media only screen and #{$declaration} {
        @content;
      }
    }
  }

  @if $breakpoint-found == false {
    @warn 'Breakpoint '#{$media-query}' does not exist';
  }
}

/* ================ Responsive Show/Hide Helper ================ */
@mixin responsive-display-helper($breakpoint: '') {
  .#{$breakpoint}show {
    display: block !important;
  }

  .#{$breakpoint}hide {
    display: none !important;
  }
}

/* ================ Responsive Text Alignment Helper ================ */
@mixin responsive-text-align-helper($breakpoint: '') {
  // stylelint:disable no-important
  .#{$breakpoint}text-left {
    text-align: left !important;
  }

  .#{$breakpoint}text-right {
    text-align: right !important;
  }

  .#{$breakpoint}text-center {
    text-align: center !important;
  }
}

@mixin visually-hidden() {
  // sass-lint:disable no-important
  position: absolute !important;
  overflow: hidden;
  width: 1px;
  height: 1px;
  margin: -1px;
  padding: 0;
  border: 0;
  clip: rect(0 0 0 0);
}

@mixin visually-shown($position: inherit) {
  // sass-lint:disable no-important
  position: $position !important;
  overflow: auto;
  width: auto;
  height: auto;
  margin: 0;
  clip: auto;
}


